<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="ForegroundBrush" Color="#e2e2e2" />
    <SolidColorBrush x:Key="AccentBrush" Color="#00e676" />
    <SolidColorBrush x:Key="LightGrayBrush" Color="#5f5f5f" />
    <SolidColorBrush x:Key="MediumGrayBrush" Color="#313131" />
    <SolidColorBrush x:Key="DarkGrayBrush" Color="#232323" />
    <SolidColorBrush x:Key="DeepDarkGrayBrush" Color="#171717" />

    <Color x:Key="ForegroundColor">#e2e2e2</Color>
    <Color x:Key="AccentColor">#00e676</Color>
    <Color x:Key="LightGrayColor">#5f5f5f</Color>
    <Color x:Key="MediumGrayColor">#313131</Color>
    <Color x:Key="DarkGrayColor">#232323</Color>
    <Color x:Key="DeepDarkGrayColor">#171717</Color>

</ResourceDictionary>